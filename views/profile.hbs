<h1>My Teams:</h1>

{{#each foundTeams}}
    <div class="teams-box">
        <div class="team-name">
            <h1>Team Name: {{teamName}}</h1>
        </div>
        <div class="poke-box">
            <div class="poke">
                <h3>{{pokemon1.name}}</h3>
                <img src="{{pokemon1.image}}" alt="name" class="pokemon-img">
            </div>
            <div class="poke">
                <h3>{{pokemon2.name}}</h3>
                <img src="{{pokemon2.image}}" alt="name" class="pokemon-img">
            </div>
            <div class="poke">
                <h3>{{pokemon3.name}}</h3>
                <img src="{{pokemon3.image}}" alt="name" class="pokemon-img">
            </div>
        </div>
        <a href="/users/update/{{_id}}"><button>Update Team</button></a>
        <a href="/users/delete/{{_id}}"><button>Delete Team</button></a>
    </div>
{{/each}}