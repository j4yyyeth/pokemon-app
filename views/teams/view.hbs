        <div class="poke-box">
            <div class="poke">
                <h3>{{pokemon1.name}}</h3>
                <img src="{{pokemon1.image}}" alt="name" class="pokemon-img">
            </div>
            <div class="poke">
                <h3>{{pokemon2.name}}</h3>
                <img src="{{pokemon2.image}}" alt="name" class="pokemon-img">
